{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/blog/*",
        "destination": "https://written-by-me.hatenadiary.jp",
        "type": 302
      }
    ],
    "rewrites": [
      {
        "source": "/quwtara-scripts/",
        "function": "scripts"
      }
    ]
  }
}
